robot_bag = []

robot_bag.append("Battery Pack")
robot_bag.append("Sensor Chip")
robot_bag.append("Robot Arm")

print("Robot's Bag contains:", robot_bag)
print("Total Items:", len(robot_bag))


import random

target_number = random.randint(1, 10)
attempts = 0

print("Apka robotic factory me swagat hai ! Suno mere robot ke battery  1 se 10 ke beech main he kya aap use guess kar sakte hai ")

while True:
    user_guess = int(input("Enter your guess:"))
    attempts += 1
    
    if user_guess < target_number:
     print("bahut chhota hai thoda bada socho")
    
    elif user_guess > target_number:
        print("bahut bada hai thoda chhota socho")
    
    else:
        print(f"Badhai ho ! apne {attempts} attempts main battery guess kiya hai ")
        break
